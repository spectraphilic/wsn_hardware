<connect gate="U1" pin="!SHUT" pad="5"/>
<connect gate="U1" pin="GND" pad="2 3 4 6 12"/>
<connect gate="U1" pin="SCL" pad="10"/>
<connect gate="U1" pin="SDA" pad="9"/>
<connect gate="U1" pin="VCC" pad="1 11"/>
